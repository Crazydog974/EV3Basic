I = 0
LCD.Clear()

Thread.Run = SUB1
Thread.Run = SUB1
Thread.Run = SUB2
Thread.Run = SUB3
Thread.Run = SUB3

Program.Delay(5000)

Sub SUB1  
  Thread.Run = SUB3
  Thread.Run = SUB3
  Thread.Run = SUB3
EndSub

Sub SUB2 
  Thread.Run = SUB3
  Thread.Run = SUB3
  Thread.Run = SUB3
EndSub

Sub SUB3
  LCD.Text(1, 40,10*I, 1, (I+1)+". call")
  I= I+1
EndSub
