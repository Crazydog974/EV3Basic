LCD.Clear()

A = 5
B = Math.Abs(-4)
C = (17-5)*(A-3)+B/2
D = (A*B)*(A+B)-((6*C)+(6*4*7*Math.Abs(-4)*3*A/12))*(4*B)-444

LCD.Text(1,0,0,  1, "A: "+(A-0))
LCD.Text(1,0,16, 1, "B: "+(B+B))
LCD.Text(1,0,32, 1, "C: "+C*3)
LCD.Text(1,0,48, 1, "D: "+(D+0))

If ((A+B)>(C+D)) THEN
  LCD.Text(1,0,64, 1, "(A+B)>(C+D)")
EndIf
If (A>(C+D)) THEN
  LCD.Text(1,0,80, 1, "A>(C+D)")
EndIf

Program.Delay(1000000)